<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.carrepair">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/cars"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Signup"
            android:label="Sign up"></activity>
        <activity
            android:name=".Incorrectcode"
            android:label="Verification Code"
            android:parentActivityName=".Sms" />
        <activity
            android:name=".Sms"
            android:label="Verification Code"
            android:parentActivityName=".Verification" />
        <activity
            android:name=".Verification"
            android:label="Verification"
            android:parentActivityName=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"/>





    </application>

</manifest>